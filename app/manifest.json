{
    "name": "Jawbone UP Chrome Extension",
    "version": "0.0.4",
    "manifest_version": 2,
    "description": "__MSG_appDescription__",
    "icons": {
        "16": "images/icon-16.png",
        "128": "images/icon-128.png"
    },
    "default_locale": "en",
    "background": {
        "page": "background.html",
        "persistent": true
    },
    "permissions": [
        "tabs",
        "background",
        "storage",
        "identity",
        "https://*.jawbone.com/*"
    ],
    "browser_action": {
        "default_icon": {
            "19": "images/icon-19.png",
            "38": "images/icon-38.png"
        },
        "default_title": "Jawbone-Chrome",
        "default_popup": "popup.html"
    }
}